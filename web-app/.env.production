VITE_APP_TITLE=Supermarket Management System
VITE_API_URL=https://api.your-domain.com/api
VITE_APP_VERSION=0.1.0
VITE_APP_ENV=production

# Configurations de l'API
VITE_API_TIMEOUT=5000
VITE_API_RETRY_COUNT=3

# Configurations de l'authentification
VITE_AUTH_STORAGE_KEY=sms_auth
VITE_TOKEN_REFRESH_INTERVAL=300000

# Configurations des fonctionnalit√©s
VITE_ENABLE_LOGS=false
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_ANALYTICS=true

# Limites
VITE_MAX_FILE_SIZE=5242880
VITE_MAX_UPLOAD_FILES=10

# Configurations des services externes
VITE_STRIPE_PUBLIC_KEY=pk_live_your_stripe_key
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_key

# Configurations du cache
VITE_CACHE_TTL=3600000
VITE_ENABLE_SERVICE_WORKER=true

# Configurations des performances
VITE_ENABLE_COMPRESSION=true
VITE_CHUNK_SIZE_WARNING=2097152
